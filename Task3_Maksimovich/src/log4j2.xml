<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

	<Appenders>

		<File name="TxtAppender" fileName="Log/log.txt">
			<PatternLayout pattern="%d{dd.MM.yyyy HH:mm:ss} [%t] %-5p %c - %msg%n" />
		</File>

	</Appenders>

	<Loggers>

		<Root level="error">
			<AppenderRef ref="TxtAppender" />
		</Root>

	</Loggers>
</Configuration>